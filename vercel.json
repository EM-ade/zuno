{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "rm -rf node_modules .next && npm cache clean --force && npm install --legacy-peer-deps",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "NODE_OPTIONS": "--max_old_space_size=4096"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=4096"
    }
  }
}
